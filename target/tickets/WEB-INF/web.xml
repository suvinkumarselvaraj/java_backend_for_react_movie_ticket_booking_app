<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>
        signup
    </servlet-name>
    <servlet-class>
        com.book.SignUp
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
        signup
    </servlet-name>
    <url-pattern>
        /signup
    </url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>
        signin
    </servlet-name>
    <servlet-class>
        com.book.SignIn
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
        signin
    </servlet-name>
    <url-pattern>
        /signin
    </url-pattern>
  </servlet-mapping>
 <!-- test servlet -->
  <servlet>
    <servlet-name>
        store
    </servlet-name>
    <servlet-class>
        com.book.SaveTickets
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
        store
    </servlet-name>
    <url-pattern>
        /store
    </url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>
        display
    </servlet-name>
    <servlet-class>
        com.book.DisplayTickets
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>
        display
    </servlet-name>
    <url-pattern>
        /display
    </url-pattern>
  </servlet-mapping>

</web-app>
